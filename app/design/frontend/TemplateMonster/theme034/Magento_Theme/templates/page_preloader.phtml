<style>
    #page-preloader {
        position: fixed;
        left: 0;
        top: 0;
        right: 0;
        bottom: 0;
        z-index: 9999;
    }
    @media print {
        #page-preloader{
            display: none;
        }
    }
    body .loading-mask .loader img,
    body .loading-mask .loader p{
        display: none;
    }

    #page-preloader .spinner,
    body .loading-mask .loader{
        position: absolute;
        z-index: 9999;
        left: 50%;
        top: 50%;
        margin: -32px 0 0 -22px;
        width: 49px;
        height: 49px;
        line-height: 90px;
        box-sizing: border-box;
        text-align: center;
        text-transform: uppercase;
        -o-text-transform: uppercase;
        -ms-text-transform: uppercase;
        -webkit-text-transform: uppercase;
        -moz-text-transform: uppercase;
    }

    #page-preloader .spinner:before,
    body .loading-mask .loader:before {
        content: '';
        width: 49px;
        height: 5px;
        background-color: #000;
        opacity: 0.5;
        position: absolute;
        top: 58px;
        left: 0;
        border-radius: 50%;
        animation: shadow 0.58s linear infinite;
        -o-animation: shadow 0.58s linear infinite;
        -ms-animation: shadow 0.58s linear infinite;
        -webkit-animation: shadow 0.58s linear infinite;
        -moz-animation: shadow 0.58s linear infinite;
    }
    @font-face {font-family: "iconfont";
            src: url('iconfont.eot?t=1528684409959'); /* IE9*/
            src: url('iconfont.eot?t=1528684409959#iefix') format('embedded-opentype'), /* IE6-IE8 */
            url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
            url('iconfont.ttf?t=1528684409959') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
            url('iconfont.svg?t=1528684409959#iconfont') format('svg'); /* iOS 4.1- */
        }
        .iconfont {
            font-family:"iconfont" !important;
            font-size: 70px;
            font-style: normal;
            color: #ff6c00;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }

        .icon-lunzi:before { content: "\e613"; }
        .wheel {
            position: absolute;
            top:-20px;
            left: -10px;
            transition: 0.5s;
            animation: cssload-animate 0.58s linear infinite;
            -o-animation: cssload-animate 0.58s linear infinite;
            -ms-animation: cssload-animate 0.58s linear infinite;
            -webkit-animation: cssload-animate 0.58s linear infinite;
            -moz-animation: cssload-animate 0.58s linear infinite;
            transform-origin: 50% 40%;
        }

    @keyframes cssload-animate {
        0%{
            transform: rotate(0);
        }
        50%{
            transform:rotate(180deg);
        }
        100%{
            transform: rotate(360deg);
        }
    }

    @-o-keyframes cssload-animate {
        0%{
            transform: rotate(0);
        }
        50%{
            transform:rotate(180deg);
        }
        100%{
            transform: rotate(360deg);
        }
    }

    @-ms-keyframes cssload-animate {
        0%{
            transform: rotate(0);
        }
        50%{
            transform:rotate(180deg);
        }
        100%{
            transform: rotate(360deg);
        }
    }

    @-webkit-keyframes cssload-animate {
        0%{
            transform: rotate(0);
        }
        50%{
            transform:rotate(180deg);
        }
        100%{
            transform: rotate(360deg);
        }
    }

    @-moz-keyframes cssload-animate {
        0%{
            transform: rotate(0);
        }
        50%{
            transform:rotate(180deg);
        }
        100%{
            transform: rotate(360deg);
        }
    }

    @keyframes shadow {
        0%,
        100% {
            transform: scale(1, 1);
        }
        50% {
            transform: scale(1.2, 1);
        }
    }

    @-o-keyframes shadow {
        0%,
        100% {
            -o-transform: scale(1, 1);
        }
        50% {
            -o-transform: scale(1.2, 1);
        }
    }

    @-ms-keyframes shadow {
        0%,
        100% {
            -ms-transform: scale(1, 1);
        }
        50% {
            -ms-transform: scale(1.2, 1);
        }
    }

    @-webkit-keyframes shadow {
        0%,
        100% {
            -webkit-transform: scale(1, 1);
        }
        50% {
            -webkit-transform: scale(1.2, 1);
        }
    }

    @-moz-keyframes shadow {
        0%,
        100% {
            -moz-transform: scale(1, 1);
        }
        50% {
            -moz-transform: scale(1.2, 1);
        }
    }
</style>

<script type="text/javascript">
    window.onload = function() {

        function fadeOut(el){
            el.style.opacity = 1;

            var last;
            var tick = function() {
                el.style.opacity = +el.style.opacity - (new Date() - last) / 600;
                last = +new Date();

                if (+el.style.opacity > 0) {
                    (window.requestAnimationFrame && requestAnimationFrame(tick)) || setTimeout(tick, 100);
                } else{
                    el.style.display = "none";
                }
            };
            tick();
        }

        var pagePreloaderId = document.getElementById("page-preloader");
        setTimeout(
            function () {
                fadeOut(pagePreloaderId)
            }, 1000);
    };
</script>

<div id="page-preloader">
    <div class="spinner">
    <div class="wheel">
               <i class="iconfont icon-lunzi"></i>
           </div>
    </div>
</div>